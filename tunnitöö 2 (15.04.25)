from tkinter import *

raam = Tk()
raam.title("Pildid")
tahvel = Canvas(raam, width=400, height=400, background = "white")
tahvel.grid()

Nimetu = PhotoImage(file="Nimetu.png")
img = tahvel.create_image(250, 80, image=Nimetu)

Nimetu2 = PhotoImage(file="Nimetu2.png")
img = tahvel.create_image(50,200,image=Nimetu2, activeimage = Nimetu, anchor=NW)

raam.mainloop()